<Page
    x:Class="Favor.MissionWrite"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Favor"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
    <Page.BottomAppBar>
        <CommandBar Background="#FFFF8F3D">
            <AppBarButton x:Name="SaveButton" Icon="Accept" Label="appbarbutton" Click="ButtonSave_Click"/>
            <AppBarButton x:Name="PushingButton" Label="Pushing" Icon="SetTile" Click="PushingButton_Click"></AppBarButton>
            <AppBarButton Icon="Cancel" Label="appbarbutton" Click="AppBarButton_Click"/>
            
        </CommandBar>
    </Page.BottomAppBar>

    <Grid Background="#FFF9F0EC">
        <Grid HorizontalAlignment="Left" Height="85" Margin="0,0,0,495" VerticalAlignment="Bottom" Width="400" Background="#FFFF8F3D"/>
        <TextBox x:Name="TextInput" HorizontalAlignment="Left" Margin="0,85,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="400" Height="495" Background="#FFF9F0EC"/>

    </Grid>
</Page>
